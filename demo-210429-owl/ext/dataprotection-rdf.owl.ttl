    rdfs:label "Objection"@en ;
    rdfs:comment "The data subjects' objection to the processing of personal data, on grounds relating to their particular situation, at any time to the processing of personal data which is based on points (d), (e) and (f) of Article 6(1)."@en ;
    rdfs:subClassOf <http://www.uni.lu/dataprotection#OWLClassImpl_58cf8e1b_e912_4f83_bdbe_0214e093bd4c> ;
