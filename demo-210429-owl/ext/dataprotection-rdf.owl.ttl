<http://www.uni.lu/dataprotection#OWLObjectProperty_570763f7_9a02_4787_bf15_dea159241d52> a :InverseFunctionalProperty,
        :ObjectProperty ;
    rdfs:label "processingHasRequirement"@en ;
    rdfs:domain <http://www.uni.lu/dataprotection#OWLClass_080c5ed8_aa3c_4bfd_af54_dc6e2a546d8b> ;
