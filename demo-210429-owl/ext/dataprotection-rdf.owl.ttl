    rdfs:subClassOf <http://www.uni.lu/dataprotection#OWLClass_f993be7a_344f_4fee_9a35_75f1e3b367c5> .

<http://www.uni.lu/dataprotection#OWLClass_f9140510_7d3e_441d_a8cf_a67da4fb19c4> a :Class ;
    rdfs:label "AutomatedProcessing"@en ;
    rdfs:comment "Processing that does not require human activity."@en ;
    rdfs:subClassOf <http://www.uni.lu/dataprotection#OWLClass_847950cb_b447_4f15_bb85_ed45f878d760> .

<http://www.uni.lu/dataprotection#OWLClass_fccddccc_e1c5_425a_9e67_99ea9c2dfa55> a :Class ;
    rdfs:label "SpecialConsent"@en ;
    rdfs:comment "Consent given under special circumstances, such as to the processing of personal data of a child or to the processing of sensitive data."@en ;
    rdfs:subClassOf <http://www.uni.lu/dataprotection#OWLClass_a2ccd29b_a341_4659_919a_977a6a81d4d1> ;
    :equivalentClass [ a :Restriction ;
